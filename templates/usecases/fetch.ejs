import { IApp } from '@/types/nuxt'
import RefreshUseCase from '@/usecases/RefreshUseCase'

export default class Fetch<%= name %>sUseCase extends RefreshUseCase implements BaseUseCase {
  app: IApp
  constructor(app: IApp) {
    super()
    this.app = app
  }

  async execute() {
    await super.refresh(this.app)
    const response = await this.app.<%= appName.toLowerCase() %>Gateway.<%= name %>.Fetch<%= name %>s(this.app.<%= name.toLowerCase() %>.options)
    this.app.<%= name.toLowerCase() %>.store(response.items)
    this.app.<%= name.toLowerCase() %>.query = response.query
    this.app.<%= name.toLowerCase() %>.options = {
      ...this.app.<%= name.toLowerCase() %>.options,
      ...response.options
    }
  }
}
